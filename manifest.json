{
  "manifest_version": 3,
  "name": "Tatarus YT Downloader",
  "version": "1.2.0",
  "description": "Download YouTube videos & audio in local device | ดาวน์โหลดวิดีโอและเพลงจาก YouTube บนอุปกรณ์ของคุณโดยไม่ประมวลผลบนเซิร์ฟเวอร์",
  "author": "Tatarus",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "default_title": "Tatarus YT Downloader"
  },
  "permissions": [
    "activeTab",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "*://www.youtube.com/*",
    "*://youtube.com/*"
  ]
}